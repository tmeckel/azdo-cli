  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug ACC Test",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}",
      "args": ["-test.run", "^${selectedText}$"],
      "env": {
        "AZDO_ACC_TEST": "1",
        "AZDO_DEBUG": "1"
      },
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Debug ACC Test (Proxy)",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}",
      "args": ["-test.run", "^${selectedText}$"],
      "env": {
        "AZDO_ACC_TEST": "1",
        "AZDO_DEBUG": "1",
        "HTTPS_PROXY": "http://127.0.0.1:8080",
        "HTTP_PROXY": "http://127.0.0.1:8080"
      },
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
